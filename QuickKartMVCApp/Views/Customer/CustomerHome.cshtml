
@{
    ViewBag.Title = "CustomerHome";
    Layout = "~/Views/Shared/_LayoutCustomer.cshtml";
}
<h3 class="text-center">Customer Home Page</h3>
<h4>Welcome @Request.QueryString["user"]</h4>
<div class="row">
    <div class="col-md-9"></div>
    <div class="cod-md-3">
         @{
             if (TempData["lastLogin"] != null)
             {
                <h3>Your last login was at @TempData["lastLogin"].ToString();</h3>
             }
             else
             {
                <p>No Previous Logins</p>
            } }
    </div>
</div>
<div class="row">
    
    <div class="col-md-4">
        @Html.Partial("_RecentPurchases")
    </div>
    <div class="col-md-8">
    </div>
</div>

